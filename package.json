{
  "name": "autocrud-core",
  "version": "0.1.2",
  "private": false,
  "type": "module",
  "description": "Auto-generate CRUD REST + GraphQL endpoints from schemas (MVP1)",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx examples/demo.ts",
    "dev:server": "tsx examples/server.ts",
    "demo": "tsx examples/demo.ts",
    "build": "tsup src/index.ts --dts --format cjs,esm --sourcemap",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.{ts,tsx}\" || echo 'eslint not configured'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier -w .",
    "format:check": "prettier -c ."
  },
  "keywords": [
    "crud",
    "rest",
    "graphql",
    "autocrud",
    "node",
    "typescript"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaibhavr2107/autocrud.git"
  },
  "bugs": {
    "url": "https://github.com/vaibhavr2107/autocrud/issues"
  },
  "homepage": "https://github.com/vaibhavr2107/autocrud#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "dependencies": {
    "ajv": "^8.17.1",
    "apollo-server-express": "^3.13.0",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "lru-cache": "^10.4.3",
    "mongodb": "^6.8.0",
    "pg": "^8.12.0",
    "pino": "^9.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  }
}
